from machine import Pin 
from time import sleep
from machine import UART

uart = UART(2, baudrate=9600, tx=17, rx=16)
# ESP32 Pin assignment 

temp = "temp"
hum = "hum"              
while True:
    msg = str(uart.read()).strip("b'\'")
    if msg == "None":
        pass
    else:
        print(str(msg)) 
    
